pipeline {
	agent { label 'master' }
	stages {
		stage('Build') {
			steps {
				sh 'echo "this is to build"'
			}
		}
		stage('Test') {
			steps {
				sh '''
					sleep 5
					echo "to test the build"
				'''
				}
			}
		stage('Deploy') {
				steps {
				sh '''
				sleep 10
				echo "This is deploy stage"
				'''
				}
			}
		}
	}
